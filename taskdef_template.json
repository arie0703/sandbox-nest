{
  "executionRoleArn": "arn:aws:iam::906124551921:role/sandbox-cicd-task-execution-role",
  "containerDefinitions": [
    {
      "name": "sandbox-cicd",
      "image": "<IMAGE_NAME>",
      "essential": true,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "environment": [
        {
          "name": "ENV",
          "value": "$ENV"
        },
        {
          "name": "AWS_LOCALSTACK_REGION",
          "value": "ap-northeast-1"
        },
        {
          "name": "AWS_LOCALSTACK_ENDPOINT",
          "value": "$AWS_LOCALSTACK_ENDPOINT"
        },
        {
          "name": "SQS_QUEUE_URL",
          "value": "$SQS_QUEUE_URL"
        },
        {
          "name": "SLACK_WORKFLOW_URL",
          "value": "$SLACK_WORKFLOW_URL"
        },
        {
          "name": "PORT",
          "value": "80"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "sandbox-cicd",
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
        "awslogs-group": "firelens-container",
        "awslogs-region": "ap-northeast-1",
        "awslogs-create-group": "true",
        "awslogs-stream-prefix": "firelens"
    }
  }
}
