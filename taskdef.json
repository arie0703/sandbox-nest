{
  "executionRoleArn": "arn:aws:iam::906124551921:role/sandbox-cicd-task-execution-role",
  "containerDefinitions": [
    {
      "name": "sandbox-cicd",
      "image": "<IMAGE_NAME>",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networdMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "sandbox-cicd"
}
